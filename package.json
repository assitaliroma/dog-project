    "debug": "^4.1.0",
    "eslint": "^5.8.0",
    "eslint-config-recommended": "^4.0.0",
    "jest": "^23.6.0",
    "yargs": "^12.0.2"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
